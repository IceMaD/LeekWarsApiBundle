{
  "name": "icemad/leek-wars-api-bundle",
  "type": "symfony-bundle",
  "description": "IceMaD LeekWarsApiBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Duboc",
      "homepage": "https://github.com/IceMaD"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/dependency-injection": "~3.4|~4.0",
    "symfony/http-kernel": "~3.4|~4.0",
    "symfony/serializer-pack": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "symfony/console": "^4.2",
    "symfony/dotenv": "^4.2",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "^4.2",
    "symfony/yaml": "^4.2"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IceMaD\\LeekWarsApiBundle\\": "src/",
      "Dev\\": "dev/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
