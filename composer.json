{
  "name": "icemad/leek-wars-api-bundle",
  "type": "symfony-bundle",
  "description": "IceMaD LeekWarsApiBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Duboc",
      "homepage": "https://github.com/IceMaD"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/flex": "^1.1",
    "symfony/http-kernel": "~3.4|~4.0",
    "symfony/serializer-pack": "^1.0"
  },
  "require-dev": {
    "symfony/dependency-injection": "~3.4|~4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IceMaD\\Bundle\\LeekWarsApiBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
